/*
 * Tabs
 */

ul.primary li a,
ul.primary li a .tab,
ul.secondary li a,
ul.secondary li a .tab {
  cursor: pointer;
}

/*
 * Drupal nodes
 */

.node-unpublished>*,
.comment-unpublished>* {
  position: relative; /* Otherwise these elements will appear below the "Unpublished" text. */
}
.searchInput{
	font-size: 10px;
	padding:3px;
	width:215px;
}
/*
 * Drupal admin tables
 */
tr.even th,
tr.even td,
tr.odd th,
tr.odd td {
  border-bottom: 1px solid #ccc; /* IE doesn't display borders on table rows */
}


